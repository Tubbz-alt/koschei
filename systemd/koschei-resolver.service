[Unit]
Description=Koschei - Fedora Continuous Integration - dependency resolver
After=syslog.target network.target

[Service]
Type=simple
ExecStart=/usr/libexec/koschei/koschei-resolver -m koschei.main resolver
User=koschei
Group=koschei
Nice=5
Restart=on-failure

[Install]
WantedBy=multi-user.target
